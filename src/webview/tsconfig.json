{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "types": ["vscode-webview"],
    "jsx": "react-jsx"
  },
  "include": ["./**/*", "../shared/**/*"],
  "exclude": ["node_modules"]
}
